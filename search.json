---
layout: null
---
{% assign titled_pages = site.pages | where_exp: "page", "page.title" %}
[{% for page in titled_pages %}
  {
    "title": {{ page.title | jsonify }},
    "url": {{ page.url | relative_url | jsonify }},
    "content": {{ page.content | strip_html | strip_newlines | truncatewords: 200 | jsonify }}
  }{% unless forloop.last %},{% endunless %}{% endfor %}
]
